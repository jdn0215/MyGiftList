<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fondo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#b0fd9e"
    tools:context="a2017iciclo.moviles.unacr.globales.proyecto.mygiftlist.mygiftlist.integrantes">

    <TableLayout
        android:layout_width="297dp"
        android:layout_height="504dp"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="16dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/dani"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Daniel Salas M. 115850529"
                android:textColor="#000"
                tools:layout_editor_absoluteX="186dp"
                tools:layout_editor_absoluteY="202dp" />

            <ImageView
                android:id="@+id/idani"
                android:layout_width="79dp"
                android:layout_height="98dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/icris"
                android:layout_marginEnd="14dp"
                android:layout_marginRight="14dp"
                android:layout_marginTop="36dp"
                app:srcCompat="@drawable/daniel"
                tools:layout_editor_absoluteX="29dp"
                tools:layout_editor_absoluteY="163dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/naza"
                android:layout_width="126dp"
                android:layout_height="24dp"
                android:text="Nazarteth Alfaro N. 604220440"
                android:textColor="#000"
                tools:layout_editor_absoluteX="186dp"
                tools:layout_editor_absoluteY="79dp" />

            <ImageView
                android:id="@+id/inaza"
                android:layout_width="71dp"
                android:layout_height="100dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/luis"
                app:srcCompat="@drawable/naza"
                tools:layout_editor_absoluteX="29dp"
                tools:layout_editor_absoluteY="38dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/luis"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Luis Rodriguez C. 115910193"
                android:textColor="#000"
                tools:layout_editor_absoluteX="186dp"
                tools:layout_editor_absoluteY="317dp" />

            <ImageView
                android:id="@+id/iluis"
                android:layout_width="83dp"
                android:layout_height="95dp"
                android:layout_alignBottom="@+id/idani"
                android:layout_alignEnd="@+id/luis"
                android:layout_alignRight="@+id/luis"
                android:layout_marginBottom="23dp"
                app:srcCompat="@drawable/luis"
                tools:layout_editor_absoluteX="29dp"
                tools:layout_editor_absoluteY="283dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/cris"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:text="Cristian Sequeira S. 207400167"
                android:textColor="#000"
                tools:layout_editor_absoluteX="186dp"
                tools:layout_editor_absoluteY="437dp" />

            <ImageView
                android:id="@+id/icris"
                android:layout_width="83dp"
                android:layout_height="83dp"
                android:layout_below="@+id/naza"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="26dp"
                app:srcCompat="@drawable/cris"
                tools:layout_editor_absoluteX="29dp"
                tools:layout_editor_absoluteY="405dp" />

        </TableRow>
    </TableLayout>

</android.support.constraint.ConstraintLayout>
